<% if(typeof errors != 'undefined') { %>
    <% errors.forEach(function(error) { %>
            <p id="err_message"><%= error.msg %></p>
    <%  }); %>
<% } %>

<% if(success_msg != '')  { %>
        <p id="succ_message"><%= success_msg %></p>
<% } %>

<% if(error_msg != '')  { %>
    <p id="err_message"><%= error.msg %></p>
<% } %>

<% if(error != '')  { %>
    <p id="err_message"><%= error %></p>
<% } %>